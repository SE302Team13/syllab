                    <th style="background-color: #cccccc;">Course Name</th>
                    <td id="course_name">{$courseName}</td>
                </tr>
            </table>
            <br /><br />
            <table class="a-table">
                <tr>
                    <th style="background-color: #cccccc;">Code</th>
                    <th style="background-color: #cccccc;">Fall<br></th>
                    <th style="background-color: #cccccc;">Spring<br></th>
                    <th style="background-color: #cccccc;">Theory<br>(hour/week)</th>
                    <th style="background-color: #cccccc;">Application/Lab<br>(hour/week)</th>
                    <th style="background-color: #cccccc;">Local<br>Credits</th>
                    <th style="background-color: #cccccc;">ECTS</th>
                </tr>
                <tr>
                    <td id="course_code">{$code}</td>
                    <td id="fall">{$fall}</td>
                    <td id="spring">{$spring}</td>
                    <td id="theory_hour">{$theory_hour}</td>
                    <td id="application_hour">{$application_hour}</td>
                    <td id="local_credits">{$local_credits}</td>
                    <td id="ects">{$ects}</td>
                </tr>
            </table>
            <br /><br />
            <table class="a-table">
                <tr>
                    <th style="background-color: #cccccc; width: 35%;">Prerequisites</th>
                    <td id="prerequisites">
                    	{% if ($prerequistes) %}
    					<br/>
    					<ul>
    						{% loop in $prerequistes as $prerequiste %}
  							<li>{$prerequiste}</li>
  							{% onEmpty %}
  							-
    						{% endloop %}
    						
  						{% endif %}
  						</ul>
  					</td>
                </tr>
                <tr>
                    <th style="background-color: #cccccc;">Course Language</th>
                    <td id="language">
                        &nbsp {$courseLang}
                    </td>
                </tr>
                <tr id="course_type">
                    <th style="background-color: #cccccc;">Course Type</th>
                    <td>&nbsp {$type}</td>
                </tr>
                <tr id="course_level">
                    <th style="background-color: #cccccc;">Course Level </th>
                    <td>&nbsp {$level}</td>
                </tr>
            </table>
            <br /><br />
