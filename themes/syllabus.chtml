                <tr>
                    <th style="background-color: #cccccc; width: 35%;">Course Coordinator</th>
                    <td id="Coordinator">&nbsp {$courseCoordinator}</td>
                </tr>
                <tr>
                    <th style="background-color: #cccccc;">Course Lecturer(s)</th>
                    <td id="lecturer_list">
                        {% if ($courseLecturers) %}
    					<br/>
    					<ul>
    						{% loop in $courseLecturers as $lecturer %}
  							<li>{$lecturer}</li>
  							{% onEmpty %}-
    						{% endloop %}
    					</ul>
  						{% endif %}
                    </td>
                </tr>
                <tr>
                    <th style="background-color: #cccccc;">Assistant(s) </th>
                    <td id="assistant_list"> {% if ($assistants) %}
    					<br/>
    					<ul>
    						{% loop in $assistants as $assistant %}
  							<li>{$assistant}</li>
  							{% onEmpty %}-
    						{% endloop %}
    					</ul>
